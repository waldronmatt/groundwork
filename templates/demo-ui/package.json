{
  "name": "@waldronmatt/demo-ui",
  "version": "3.5.19",
  "type": "module",
  "exports": {
    "./lib/*": "./lib/*"
  },
  "publishConfig": {
    "access": "public",
    "types": "./dist/types/esm/index.d.ts",
    "module": "./dist/esm/index.js",
    "main": "./dist/cjs/index.cjs",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/types/esm/index.d.ts",
          "default": "./dist/esm/index.js"
        },
        "require": {
          "types": "./dist/types/cjs/index.d.cts",
          "default": "./dist/cjs/index.cjs"
        }
      },
      "./*": {
        "import": {
          "types": "./dist/types/esm/*",
          "default": "./dist/esm/*"
        },
        "require": {
          "types": "./dist/types/cjs/*",
          "default": "./dist/cjs/*"
        }
      },
      "./styles/*": {
        "import": "./dist/styles/*",
        "require": "./dist/styles/*"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "lint:knip": "knip --exports",
    "lint:types": "attw --from-npm @waldronmatt/demo-ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build": "tsc --p ./tsconfig.build.json && vite build",
    "clean": "rimraf dist tsconfig.build.tsbuildinfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waldronmatt/groundwork.git",
    "directory": "templates/demo-ui"
  },
  "author": "Matthew Waldron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waldronmatt/groundwork/issues"
  },
  "homepage": "https://github.com/waldronmatt/groundwork/tree/main/templates/demo-ui#readme",
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "20.17.10",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@vitejs/plugin-react-swc": "3.7.2",
    "eslint": "8.57.1",
    "eslint-config-custom": "workspace:*",
    "fs-extra": "11.2.0",
    "glob": "10.4.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "5.0.10",
    "tsconfig-config": "workspace:*",
    "typescript": "5.7.2",
    "vite": "5.4.14",
    "vite-plugin-dts": "3.9.1",
    "vite-plugin-lib-inject-css": "1.3.0",
    "vite-plugin-static-copy": "1.0.6"
  }
}
