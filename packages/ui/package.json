{
  "name": "@waldronmatt/demo-ui",
  "version": "3.1.2",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "require": "./dist/cjs/*",
      "import": "./dist/esm/*",
      "types": "./dist/types/*"
    },
    "./lib/*": "./lib/*"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "tsc --p ./tsconfig.build.json && vite build",
    "clean": "rimraf dist coverage tsconfig.build.tsbuildinfo"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waldronmatt/groundwork.git",
    "directory": "packages/ui"
  },
  "author": "Matthew Waldron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waldronmatt/groundwork/issues"
  },
  "homepage": "https://github.com/waldronmatt/groundwork/tree/main/packages/ui#readme",
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.11",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react-swc": "3.5.0",
    "eslint": "8.56.0",
    "eslint-config-custom": "workspace:*",
    "glob": "10.3.10",
    "jest": "29.7.0",
    "jest-config": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.5",
    "tsconfig-config": "workspace:*",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-plugin-dts": "3.7.1",
    "vite-plugin-lib-inject-css": "1.3.0",
    "vite-plugin-static-copy": "1.0.0"
  }
}
