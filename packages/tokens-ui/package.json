{
  "name": "@waldronmatt/story-tokens-ui",
  "version": "1.1.8",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/types/*",
      "import": "./dist/*"
    },
    "./lib/*": "./lib/*"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build": "tsc --p ./tsconfig.build.json && vite build",
    "clean": "rimraf dist coverage tsconfig.build.tsbuildinfo"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waldronmatt/groundwork.git",
    "directory": "packages/story-tokens-ui"
  },
  "author": "Matthew Waldron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waldronmatt/groundwork/issues"
  },
  "homepage": "https://github.com/waldronmatt/groundwork/tree/main/packages/story-tokens-ui#readme",
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.6.17"
  },
  "devDependencies": {
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.23",
    "@vitejs/plugin-react-swc": "3.6.0",
    "eslint": "8.57.0",
    "eslint-config-custom": "workspace:*",
    "glob": "10.3.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.5",
    "storybook": "7.6.17",
    "tsconfig-config": "workspace:*",
    "typescript": "5.4.3",
    "vite": "5.1.7",
    "vite-plugin-dts": "3.7.3",
    "vite-plugin-lib-inject-css": "1.3.0"
  }
}
